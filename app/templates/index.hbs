
<div class="row">
  <div class="well">
    <h1>WallBlog</h1>
    <h2>Post your blogs!</h2>
  </div>
  <div class="well">
    <div class="">
        <h1>Displaying Blog</h1>
        {{#each model as |blog|}}
        <div class="well">
          <h4>ID: {{blog.id}}</h4>
          <h4>Name: {{blog.name}}</h4>
          <h4>Location: {{blog.location}}</h4>
          <h4>Content: {{blog.content}}</h4>
        </div>
        {{/each}}
    </div>
    <div class="">
      <h1>Blog 2</h1>
    </div>
    <div class="">
      <h1>Blog 3</h1>
    </div>
    <div class="">
      <h1>Blog 4</h1>
    </div>
  </div>
</div>
